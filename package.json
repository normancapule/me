{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "gatsby": "^2.23.1",
    "gatsby-image": "^2.4.6",
    "gatsby-plugin-emotion": "^4.3.3",
    "gatsby-plugin-manifest": "^2.4.10",
    "gatsby-plugin-offline": "^3.2.8",
    "gatsby-plugin-postcss": "^2.3.3",
    "gatsby-plugin-react-helmet": "^3.3.3",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-transformer-sharp": "^2.5.4",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "stylelint": "^13.6.0",
    "tailwindcss": "^1.4.6",
    "twin.macro": "^1.3.0",
    "typeface-ibm-plex-sans": "^0.0.75",
    "typeface-inter": "^3.12.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.12",
    "@types/react": "^16.9.35",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-markdown": "^2.0.0-alpha.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typescript": "tsc",
    "eslint:check": "eslint '**/*.{js,jsx,json,md,ts,tsx}'",
    "eslint:fix": "eslint '**/*.{js,jsx,json,md,ts,tsx}' --fix",
    "prettier:check": "prettier '**/*.{js,jsx,json,md,ts,tsx}' --list-different",
    "prettier:fix": "prettier '**/*.{js,jsx,json,md,ts,tsx}' --write",
    "ci": "run-p eslint:check prettier:check typescript",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
